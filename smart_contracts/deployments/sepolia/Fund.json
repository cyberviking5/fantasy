ructor(){\\n        i_owner = msg.sender;\\n    }\\n\\n    function fund() public payable {\\n        require(msg.value>=MINIMUM_ETHER,\\\"not enought ETH\\\");\\n        gamblersToAmountBet[msg.sender] += msg.value;\\n        gamblers.push(msg.sender);\\n    }\\n\\n    function getOwner() public view returns(address)\\n    {\\n        return i_owner;\\n    }\\n    \\n}\\n\",\"keccak256\":\"0xc70274544779b465c13cb8e911031ff49774c354ef62ad31ecd0199f00a91d4e\",\"license\":\"MIT\"}},\"version\":1}",
